

driver.get("https://alchemy.hguy.co/lms")

String heading = String (driver.findElement(By.xpath("//div[@class='ld-course-list-items row']/following-sibling::h3[2]")))
if (heading.matches(“Email Marketing Strategies”))
  System.out.print("Matched")
else
  System.out.print("Not Matched")
driver.quit()
